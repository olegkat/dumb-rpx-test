<html>
<head>
  <title>See You Later!</title>
</head>
<body>
  <% if not @signed_in -%>
    <% if RPX[:embed] -%>
  <iframe src="<%= @rpx_embed_url %>"
           scrolling="no"
           frameBorder="no"
           allowtransparency="true"
           style="width:400px;height:240px">
  </iframe>

    <% else -%>
  <p>
    <a class="rpxnow" onclick="return false;" href="<%= @rpx_signin_url %>">
    Sign in
    </a>
  </p>
    <% end -%>

  <p>
  <form action="<%= h @api[:facebook_set_app_properties] %>" method="post">
  <fieldset>
    <legend>facebook_set_app_properties</legend>
    <table>
      <tr>
        <td>fbAPIKey:</td>
        <td><input type="text" name="fbAPIKey" value="" /><td/>
      </tr>
      <tr>
        <td>fbSecret:</td>
        <td><input type="text" name="fbSecret" value="" /><td/>
      </tr>
      <tr>
        <td>fbAppID:</td>
        <td><input type="text" name="fbAppID" value="" /><td/>
      </tr>
      <tr>
        <td>emailPerm:</td>
        <td><input type="text" name="emailPerm" value="true" /><td/>
      </tr>
      <tr>
        <td>uninstallURL:</td>
        <td><input type="text" name="uninstallURL" value="" /><td/>
      </tr>
    </table>
    <input type="hidden" name="apiKey" value="<%= @api_key %>"/>
    <input type="submit" value="post" /><br/>
  </fieldset>
  </form>
  </p>


  <p>
  <form action="<%= h @api[:set_status] %>" method="post">
  <fieldset>
    <legend>set_status</legend>
    <table>
      <tr>
        <td>device_token:</td>
        <td><input type="text" name="device_token" value="4554be531fb23c1a1896bedb63c50df1f10a1d71" /><td/>
      </tr>
      <tr>
        <td>status:</td>
        <td><input type="textarea" name="status" value="test" /><td/>
      </tr>
    </table>
    <input type="submit" value="post" /><br/>
  </fieldset>
  </form>
  </p>


  <% else -%>

  <p>
    <a href="<%= @signout_url %>">
    Sign out
    </a>
  </p>


  <p>
  <form action="<%= h (@api[:auth_info] + '?format=json') %>" method="post">
  <fieldset>
    <legend>auth_info</legend>
    <table>
      <tr>
        <td>extended:</td>
        <td><input type="text" name="extended" value="true" /><td/>
      </tr>
<!--      <tr>
        <td>format:</td>
        <td><input type="text" name="format" value="json" /><td/>
      </tr>-->
    </table>
    <input type="hidden" name="apiKey" value="<%= @api_key %>"/>
    <input type="hidden" name="token" value="<%= @token %>"/>
    <input type="submit" value="post" /><br/>
  </fieldset>
  </form>
  </p>


